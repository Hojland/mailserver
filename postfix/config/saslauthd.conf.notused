ldap_servers: ldap://<domaincontroller>/
ldap_bind_dn: cn=postfix,ou=services,<addomain>
ldap_bind_pw: <secret>
ldap_timeout: 10
ldap_time_limit: 10
ldap_scope: sub
ldap_search_base: cn=Users,<addomain>
ldap_auth_method: bind
ldap_filter: (&(userPrincipalName=%u)(objectClass=person))
ldap_debug: 0
ldap_verbose: off
ldap_ssl: no
ldap_starttls: no
ldap_referrals: yes

